{
  "compilerOptions": {
      "target": "ES2022",
      "module": "nodenext",
      "moduleResolution": "nodenext",
      "strict": true,
      "noImplicitAny": true,
      "declaration": false,
      "skipLibCheck": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "rootDir": "src",
      "outDir": "dist"
  },
  "exclude": ["node_modules"]
}